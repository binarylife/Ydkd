<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bei.yd"
    >

  <application
      android:name=".app.YDApp"
      android:allowBackup="true"
      android:label="@string/app_name"
      android:icon="@mipmap/ic_launcher"
      android:supportsRtl="true"
      android:theme="@style/AppTheme"
      >
    <activity
        android:name=".ui.main.activity.SplashActivity"
        android:theme="@style/AppTheme.Launcher">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>

    <activity android:name=".ui.main.activity.MainActivity"
        android:theme="@style/BaseActivity"
        android:launchMode="singleTask"
        android:screenOrientation="portrait"/>
    <activity android:name=".ui.main.activity.AddWorkOrderActivity"
        android:screenOrientation="portrait"
        android:theme="@style/BaseActivity"/>
    <activity android:name=".ui.main.activity.LoginActivity"
        android:screenOrientation="portrait"
        android:theme="@style/BaseActivity"/>
    <activity android:name=".ui.main.activity.PaiWorkerListActivity"
        android:screenOrientation="portrait"
        android:theme="@style/BaseActivity"/>
    <activity android:name=".ui.main.activity.OrderDetialActivity"
        android:screenOrientation="portrait"
        android:theme="@style/BaseActivity"/>
  </application>

</manifest>
